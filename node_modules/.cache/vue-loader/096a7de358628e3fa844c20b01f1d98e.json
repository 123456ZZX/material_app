trolData.collaborator.split('、')\" :key=\"index\">\r\n          {{ item }}\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: {\r\n    PatrolData: {\r\n      type: Object,\r\n      default: () => {},\r\n    },\r\n    show: {\r\n      type: Boolean,\r\n      default: false,\r\n    },\r\n  },\r\n  data() {\r\n    return {}\r\n  },\r\n\r\n  created() {},\r\n\r\n  methods: {\r\n    //计算剩余时间\r\n    displayTime(timeType, type) {\r\n      let time = this.$dayjs(this.PatrolData && this.PatrolData.taskEndTime).diff(new Date(), timeType)\r\n      type ? (time = time % type) : null\r\n      console.log(time);\r\n      time < 0 ? (time = 0) : time\r\n      return time\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.box-time-img-time {\r\n  width: 32px;\r\n  height: 32px;\r\n}\r\n\r\n.box-time-img-down {\r\n  width: 32px;\r\n  height: 42px;\r\n}\r\n\r\n.inspection-status-content {\r\n  position: absolute;\r\n  top: 210px;\r\n  width: calc(100% - 60px);\r\n  padding: 20px 30px;\r\n  font-size: 28px;\r\n  color: #999;\r\n  background-color: #ffffff;\r\n  z-index: 1;\r\n\r\n  .status-content-item {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-bottom: 20px;\r\n\r\n    .item-label {\r\n      margin: 0 12px;\r\n    }\r\n\r\n    .reason {\r\n      width: 160px;\r\n    }\r\n\r\n    .reason-content {\r\n      width: calc(100% - 300px);\r\n      color: #ff4d4f;\r\n      word-wrap: break-word;\r\n      word-break: normal;\r\n    }\r\n\r\n    .icon-normal,\r\n    .icon-abnormal {\r\n      display: flex;\r\n      justify-content: center;\r\n      align-items: center;\r\n      width: 80px;\r\n      height: 40px;\r\n      background: #40bb5a;\r\n      border-radius: 38px;\r\n      color: #fff;\r\n      margin-left: 10px;\r\n    }\r\n\r\n    .icon-abnormal {\r\n      background: #ff9429;\r\n    }\r\n\r\n    .content-item-time {\r\n      display: flex;\r\n      font-size: 24px;\