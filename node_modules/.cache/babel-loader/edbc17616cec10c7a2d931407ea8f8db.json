a)\r\n    storage.set('portal', JSON.stringify(data))\r\n  },\r\n  [types.CLEAN_VIEWS]: state => {\r\n    // not keep affix tags\r\n    state[stateTypes.VISITED_VIEWS] = []\r\n    state[stateTypes.CACHED_VIEWS] = []\r\n  },\r\n  [types.CLEAN_ROUTE]: (state) => {\r\n    state[stateTypes.ROUTES] = []\r\n    resetRouter()\r\n  },\r\n  [types.CLEAN_PERMISSION]: (state) => {\r\n    state[stateTypes.PERMISSION] = {}\r\n  },\r\n  [types.CLEAN_PERMISSION_QUEUE]: (state, code) => {\r\n    state[stateTypes.PERMISSION_QUEUE] = new Set()\r\n  },\r\n  [types.SET_CLIENT_CONFIG]: (state, data) => {\r\n    state[stateTypes.ADVANCED_CONFIG] = { ...state[stateTypes.ADVANCED_CONFIG], ...(additionalInformation?.advancedConfig || {}) }\r\n  }\r\n}\r\n\r\nexport default mutations\r\n"],"mappings":"AAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,OAAO,KAAKA,KAAK,MAAM,iBAAiB;AACxC,OAAO,KAAKC,UAAU,MAAM,qBAAqB;AACjD,SAASC,WAAW,QAAQ,UAAU;AACtC,OAAOC,OAAO,MAAM,iBAAiB;AACrC,OAAOC,SAAS,MAAM,WAAW;AACjC,MAAMC,SAAS,GAAG;EAChB,CAACL,KAAK,CAACM,qBAAqB,GAAG,CAACC,KAAK,EAAEC,IAAI,KAAK;IAC9CD,KAAK,CAACN,UAAU,CAACQ,iBAAiB,CAAC,GAAGD,IAAI;EAC5C,CAAC;EACD,CAACR,KAAK,CAACU,aAAa,GAAG,CAACH,KAAK,EAAEC,IAAI,KAAK;IACtCD,KAAK,CAACN,UAAU,CAACU,SAAS,CAAC,GAAGH,IAAI;EACpC,CAAC;EACD;EACA,CAACR,KAAK,CAACY,QAAQ,GAAG,CAACL,KAAK,EAAEC,IAAI,KAAK;IAAG;IACpCD,KAAK,CAACN,UAAU,CAACY,IAAI,CAAC,GAAGC,MAAM,CAACC,MAAM,CAAC,CAAC,CAAC,EAAER,KAAK,CAACN,UAAU,CAACY,IAAI,CAAC,EAAEL,IAAI,CAAC;EAC1E,CAAC;EAED,CAACR,KAAK,CAACgB,eAAe,GAAG,CAACT,KAAK,EAAEC,IAAI,KAAK;IACxCD,KAAK,CAACN,UAAU,CAACgB,WAAW,CAAC,GAAGT,IAAI;EACtC,CAAC;EACD,CAACR,KAAK,CAACkB,mBAAmB,GAAG,CAACX,KAAK,EAAEC,IAAI,KAAK;IAC5CD,KAAK,CAACN,UAAU,CAACkB,eAAe,CAAC,GAAGX,IAAI;EAC1C,CAAC;EACD,CAACR,KAAK,CAACoB,kBAAkB,GAAG,CAACb,KAAK,EAAEC,IAAI,KAAK;IAC3CD,KAAK,CAACN,UAAU,CAACoB,cAAc,CAAC,GAAGb,IAAI;EACzC,CAAC;EACD,CAACR,KAAK,CAACsB,qBAAqB,GAAG,CAACf,KAAK,EAAEC,IAAI,KAAK;IAC9CD,KAAK,CAACN,UAAU,CAACsB,iBAAiB,CAAC,GAAGf,IAAI;EAC5C,CAAC;EACD,CAACR,KAAK,CAACwB,aAAa,GAAG,CAACjB,KAAK,