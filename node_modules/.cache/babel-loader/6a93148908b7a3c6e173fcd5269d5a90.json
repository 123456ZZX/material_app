\"hiddenDangerShow\" position=\"bottom\" :style=\"{ height: '30%' }\">\r\n    <div class=\"hidden-danger\">\r\n      <div\r\n        class=\"hidden-danger-item\"\r\n        v-for=\"(item, index) in hiddenDangerList\"\r\n        :key=\"index\"\r\n        @click=\"workerDetails(item)\"\r\n      >\r\n        <div class=\"item-title\">\r\n          <img src=\"@/assets/img/pump/inspection-hidden-danger.png\" class=\"item-title-img\" alt=\"\" />\r\n          <div class=\"item-title-status\">\r\n            {{ getTicketStatusName( item.ticketStatus && item.ticketStatus.ticketStatus) }}\r\n          </div>\r\n        </div>\r\n        <div class=\"item-content\">\r\n          <span class=\"item-content-title\">异常位置：</span>\r\n          <span class=\"item-content-data\">{{\r\n            getPumpAndDeviceName(    item.ticketInfo && item.ticketInfo.formValue.deviceList[0].devicePart)\r\n          }}</span>\r\n        </div>\r\n        <div class=\"item-content\">\r\n          <span class=\"item-content-title\">隐患描述：</span>\r\n          <span class=\"item-content-data\">\r\n            {{   item.ticketInfo && item.ticketInfo.formValue.deviceList[0].detail }}\r\n          </span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </fm-popup>\r\n</template>\r\n\r\n<script>\r\nimport { getToken,  pumpInfoList, deviceInfoList } from '../api'\r\nexport default {\r\n  props: {\r\n    hiddenDangerList: {\r\n      type: Array,\r\n      default: () => [],\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      hiddenDangerShow: false,\r\n      pumpList: [],\r\n      deviceList: [],\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.getPumpInfoList()\r\n    this.getDeviceInfoList()\r\n  },\r\n\r\n  methods: {\r\n    show() {\r\n      this.hiddenDangerShow = true\r\n    },\r\n\r\n    //获取泵站列表\r\n    async getPumpInfoList() {\r\n      const { data } = await pumpInfoList()\r\n      this.pumpList = data\r\n    },\r\n\r\n    //获取设备列表\r\n    async getDeviceInfoList() {\r\n      const { data } = await deviceInfoList()\r\n      this.deviceList = data\r\n    },\r\n\r\n    //id翻译名称\r\n    getPumpAndDeviceName(arr) {\r\n      const [pumpId, devicId] = arr\r\n      console.log(pumpId, devicId)\r\n      let pumpName = this.pumpList?.filter((item) => {\r\n        return item.id === pumpId\r\n      })\r\n      let deviceName = this.deviceList?.filter((item) => {\r\n        return item.id === devicId\r\n      })\r\n\r\n      pumpName = pumpName ? pumpName[0]?.pumpName : ''\r\n      deviceName = deviceName ? deviceName[0]?.deviceName : ''\r\n\r\n      return `${pumpName} - ${deviceName}`\r\n    },\r\n\r\n    //工单详情\r\n    async workerDetails(item) {\r\n      let workOrderId = item.ticketInfo.id\r\n      const { accessToken } = await getToken(this.$storage.get('access_token'))\r\n\r\n      let workerDetailsUrl = `${process.env.VUE_APP_WPG_URL}/outworkApp/#/pages/ticket/detail/index?id=${workOrderId}&token=${accessToken}&sysCode=xasw-pump`\r\n      console.log(workerDetailsUrl)\r\n      window.location.href = workerDetailsUrl\r\n    },\r\n\r\n    //获取工单状态名称\r\n    // 工单状态 1办理中 2已完结 3挂起 99 作废\r\n    getTicketStatusName(type) {\r\n      switch (type) {\r\n        case '1':\r\n          return '办理中'\r\n        case '2':\r\n          return '已完结'\r\n        case '99':\r\n          return '作废'\r\n      }\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.hidden-danger {\r\n  overflow: scroll;\r\n  padding: 30px;\r\n\r\n  .hidden-danger-item {\r\n    background: #eee;\r\n    box-shadow: 0px 4px 16px 0px rgba(104, 115, 127, 0.06);\r\n    border-radius: 16px;\r\n    padding-bottom: 10px;\r\n    margin-bottom: 21px;\r\n\r\n    .item-title {\r\n      display: flex;\r\n      justify-content: space-between;\r\n      height: 44px;\r\n      margin-bottom: 24px;\r\n\r\n      .item-title-img {\r\n        width: 142px;\r\n        height: 44px;\r\n      }\r\n\r\n      .item-title-status {\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        width: 100px;\r\n        height: 42px;\r\n        font-size: 24px;\r\n        color: #1b67d8;\r\n        background-color: rgba(45, 126, 255, 0.1);\r\n        border-radius: 0px 10px 0px 10px;\r\n      }\r\n    }\r\n\r\n    .item-content {\r\n      font-size: 28px;\r\n      padding: 0 30px;\r\n      margin-bottom: 24px;\r\n\r\n      .item-content-title {\r\n        font-weight: bold;\r\n        color: #191919;\r\n      }\r\n      .item-content-data {\r\n        word-wrap: break-word;\r\n        word-break: normal;\r\n        color: #555555;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AAiCA,SAAAA,QAAA,EAAAC,YAAA,EAAAC,cAAA;AACA;EACAC,KAAA;IACAC,gBAAA;MACAC,IAAA,EAAAC,KAAA;MACAC,OAAA,EAAAA,CAAA;IACA;EACA;EACAC,KAAA;IACA;MACAC,gBAAA;MACAC,QAAA;MACAC,UAAA;IACA;EACA;EAEAC,QAAA;IACA,KAAAC,eAAA;IACA,KAAAC,iBAAA;EACA;EAEAC,OAAA;IACAC,KAAA;MACA,KAAAP,gBAAA;IACA;IAEA;IACA,MAAAI,gBAAA;MACA;QAAAL;MAAA,UAAAP,YAAA;MACA,KAAAS,QAAA,GAAAF,IAAA;IACA;IAEA;IACA,MAAAM,kBAAA;MACA;QAAAN;MAAA,UAAAN,cAAA;MACA,KAAAS,UAAA,GAAAH,IAAA;IACA;IAEA;IACAS,qBAAAC,GAAA;MAAA,IAAAC,cAAA,EAAAC,gBAAA,EAAAC,UAAA,EAAAC,YAAA;MACA,OAAAC,MAAA,EAAAC,OAAA,IAAAN,GAAA;MACAO,OAAA,CAAAC,GAAA,CAAAH,MAAA,EAAAC,OAAA;MACA,IAAAG,QAAA,IAAAR,cAAA,QAAAT,QAAA,cAAAS,cAAA,uBAAAA,cAAA,CAAAS,MAAA,CAAAC,IAAA;QACA,OAAAA,IAAA,CAAAC,EAAA,KAAAP,MAAA;MACA;MACA,IAAAQ,UAAA,IAAAX,gBAAA,QAAAT,UAAA,cAAAS,gBAAA,uBAAAA,gBAAA,CAAAQ,MAAA,CAAAC,IAAA;QACA,OAAAA,IAAA,CAAAC,EAAA,KAAAN,OAAA;MACA;MAEAG,QAAA,GAAAA,QAAA,IAAAN,UAAA,GAAAM,QAAA,iBAAAN,UAAA,uBAAAA,UAAA,CAAAM,QAAA;MACAI,UAAA,GAAAA,UAAA,IAAAT,YAAA,GAAAS,UAAA,iBAAAT,YAAA,uBAAAA,YAAA,CAAAS,UAAA;MAEA,UAAAJ,QAAA,MAAAI,UAAA;IACA;IAEA;IACA,MAAAC,cAAAH,IAAA;MACA,IAAAI,WAAA,GAAAJ,IAAA,CAAAK,UAAA,CAAAJ,EAAA;MACA;QAAAK;MAAA,UAAAnC,QAAA,MAAAoC,QAAA,CAAAC,GAAA;MAEA,IAAAC,gBAAA,MAAAC,OAAA,CAAAC,GAAA,CAAAC,eAAA,8CAAAR,WAAA,UAAAE,WAAA;MACAV,OAAA,CAAAC,GAAA,CAAAY,gBAAA;MACAI,MAAA,CAAAC,QAAA,CAAAC,IAAA,GAAAN,gBAAA;IACA;IAEA;IACA;IACAO,oBAAAxC,IAAA;MACA,QA