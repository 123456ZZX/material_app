rn '已终止'\r\n      }\r\n    },\r\n\r\n    //获取状态类型字体颜色\r\n    getStatusColor(type) {\r\n      switch (type) {\r\n        case 3:\r\n          return '#40BB5A'\r\n        case 4:\r\n          return '#FF4D4F '\r\n        default:\r\n          return '#1B67D8 '\r\n      }\r\n    },\r\n\r\n    //异常上报被点击\r\n    hiddenDangerClick(data) {\r\n      this.$emit('hiddenDangerClick', data, 'all')\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.inspection-status {\r\n  display: flex;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  height: 120px;\r\n  background-color: #fff;\r\n\r\n  .inspection-status-item {\r\n    text-align: center;\r\n    font-weight: bold;\r\n    font-size: 32px;\r\n\r\n    .status-item-value {\r\n      font-size: 28px;\r\n      font-weight: bold;\r\n      margin-bottom: 10px;\r\n    }\r\n\r\n    .status-item-label {\r\n      font-size: 24px;\r\n    }\r\n  }\r\n\r\n  .inspection-status-direction {\r\n    display: flex;\r\n    align-items: center;\r\n\r\n    .status-item-select {\r\n      margin-right: 20px;\r\n      .item-select-img {\r\n        width: 24px;\r\n        height: 24px;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AA+BA;EACAA,KAAA;IACA;IACAC,UAAA;MACAC,IAAA,EAAAC,MAAA;MACAC,OAAA,EAAAA,CAAA;IACA;EACA;EACAC,KAAA;IACA;MACAC,iBAAA;IACA;EACA;EAEAC,QAAA;EAEAC,OAAA;IACA;IACAC,YAAA;MACA,KAAAH,iBAAA,SAAAA,iBAAA;MACA,KAAAI,KAAA,qBAAAJ,iBAAA;IACA;IAEA;IACAK,mBAAA;MAAA,IAAAC,qBAAA;MACA,IAAAC,GAAA;MACA,KAAAZ,UAAA,MAAAW,qBAAA,QAAAX,UAAA,CAAAa,qBAAA,cAAAF,qBAAA,uBAAAA,qBAAA,CAAAG,OAAA,CAAAC,IAAA;QAAA,IAAAC,qBAAA;QACAD,IAAA,aAAAA,IAAA,gBAAAC,qBAAA,GAAAD,IAAA,CAAAE,qBAAA,cAAAD,qBAAA,eAAAA,qBAAA,CAAAF,OAAA,CAAAV,IAAA;UACAA,IAAA,CAAAc,WAAA,GAAAN,GAAA;QACA;MACA;MACA,OAAAA,GAAA;IACA;IAEA;IACAO,kBAAAlB,IAAA;MACA,QAAAA,IAAA;QACA;UACA;QACA;UACA;QACA;UACA;QACA;UACA;MACA;IACA;IAEA;IACAmB,eAAAnB,IAAA;MACA,QAAAA,IAAA;QACA;UACA;QACA;UACA;QACA;UACA;MACA;IACA;IAEA;IACAoB,kBAAAjB,IAAA;MACA,KAAAK,KAAA,sBAAAL,IAAA;I